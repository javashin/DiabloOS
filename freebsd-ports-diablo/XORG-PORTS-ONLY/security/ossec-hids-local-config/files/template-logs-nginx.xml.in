<?xml version="1.0" encoding="UTF-8"?>
<template_config os="FreeBSD" profile="%%LOGS_NGINX_PROFILE%%">

  <localfile>
    <log_format>apache</log_format>
    <location>/var/log/nginx/error.log</location>
  </localfile>

  <localfile>
    <log_format>apache</log_format>
    <location>/var/log/nginx/access.log</location>
  </localfile>

</template_config>

